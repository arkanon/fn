minset(lastn,funcao,intx) := block
(
  [
    i   : 2,
    xm  : 0,
    ym  : 0,
    set : [],
    fi,
    di
  ],
  while (i<=lastn) do
  (
    fi  : apply(funcao,[i,x]),
    di  : diff(fi,x),
    xm  : find_root(di,x,intx[1],intx[2]),
    ym  : ev(fi,x=xm),
    set : append(set,[[xm,ym]]),
    i   : i+2
  ),
  set
)$
